{
  "name": "cucumber-protractor",
  "version": "2.5.0",
  "description": "POM CukeTractor - Bootstrap your cucumber tests with cucumber, protractor and a structured way of creating page objects and component objects",
  "main": "index.js",
  "scripts": {
    "build": "react-scripts build",
    "ci": "npm run install-chromedriver && npm run start & npm run lint && npm run uitests",
    "eject": "react-scripts eject",
    "lint": "eslint '**/*.js'",
    "build-snippets:atom": "node scripts/generateSnippetsAtom.js --genFiles",
    "build-snippets:sublime": "node scripts/generateSnippetsSublime.js --genFiles",
    "build-snippets:vscode": "node scripts/generateSnippetsVSCode.js --genFiles",
    "build-snippets:webstorm": "node scripts/generateSnippetsWebstorm.js --genFiles",
    "build-snippets:intellij": "node scripts/generateSnippetsIntelliJ.js --genFiles",
    "build-readme": "node scripts/generateStepDefinitionsREADME.js --genFiles",
    "build-readme-and-snippets": "rm -rf snippets && npm run build-snippets:atom && npm run build-snippets:sublime && npm run build-snippets:vscode && npm run build-snippets:webstorm && npm run build-snippets:intellij && npm run build-readme",
    "install-chromedriver": "./node_modules/protractor/node_modules/webdriver-manager/bin/webdriver-manager update --gecko=false --standalone=false --versions.chrome 2.35",
    "install-geckodriver": "./node_modules/protractor/node_modules/webdriver-manager/bin/webdriver-manager update --chrome=false --standalone=false",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "uitests": "node index.js",
    "ct": "PATH=$(npm bin):$PATH NODE_OPTIONS=--no-deprecation cuketractor",
    "cuketractor": "PATH=$(npm bin):$PATH NODE_OPTIONS=--no-deprecation cuketractor"
  },
  "bin": {
    "uitests": "./index.js",
    "cuketractor": "./index.js",
    "brmbrm": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/canvaspixels/cucumber-protractor.git"
  },
  "author": "Alex Rogers",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cli-table": "^0.3.1",
    "colors": "^1.3.1",
    "cucumber": "^4.2.1",
    "cucumber-html-reporter": "^4.0.3",
    "jsonfile": "^4.0.0",
    "ncp": "^2.0.0",
    "protractor": "^5.4.0",
    "protractor-cucumber-framework": "^5.0.0",
    "yaml-page-objects": "0.0.0",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4"
  }
}
